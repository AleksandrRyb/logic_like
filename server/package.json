    "express": "^5.1.0",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.0",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.5",
