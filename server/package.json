  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --clear --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "node dist/db/seed.js",
    "lint": "biome lint --apply src",
