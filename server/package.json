    "drizzle:migrate": "drizzle-kit migrate",
    "seed": "ts-node-dev --transpile-only src/db/seed.ts",
    "test": "echo \"No tests\" && exit 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
